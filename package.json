{
  "name": "static-site-scaffold",
  "version": "1.0.0-1",
  "description": "Eleventy based static site scaffolding",
  "main": "lib/11ty.config.js",
  "contributors": [
    "Sam Richard <snugug@google.com> (https://snugug.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chromeos/static-site-scaffold.git"
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md",
    "CODE_OF_CONDUCT.md",
    "config/**/*",
    ".github/CONTRIBUTING.md"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "pretest": "run-p lint",
    "test:lint": "eslint tests",
    "js:lint": "eslint lib tests",
    "lint": "NODE_ENV=production run-p test:lint js:lint",
    "test": "nyc ava"
  },
  "license": "Apache-2.0",
  "ava": {
    "files": [
      "tests/**/*"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "all": true
  },
  "peerDependencies": {
    "gulp": "^4.0.2",
    "@11ty/eleventy": "^0.8.3",
    "rollup": "^1.19.4"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "eslint": "^6.3.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@emmetio/expand-abbreviation": "^0.7.3",
    "browser-sync": "^2.26.7",
    "config": "^3.2.4",
    "critical": "^1.3.6",
    "gulp-cached": "^1.1.1",
    "gulp-eslint": "^6.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "js-yaml": "^3.13.1",
    "lazypipe": "^1.0.2",
    "markdown-it": "^9.1.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-attrs": "^3.0.1",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "node-sass": "^4.12.0",
    "prismjs": "^1.17.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "workbox-build": "^4.3.1"
  }
}
