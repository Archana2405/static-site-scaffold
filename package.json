    "gulp-sourcemaps": "^2.6.5",
    "http-server": "^0.11.1",
    "husky": "^3.0.5",
    "lighthouse": "^5.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
