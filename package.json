    "babel-register": "^6.26.0",
    "breakpoint-sass": "^2.7.1",
    "browser-sync": "^2.26.7",
    "chrome-launcher": "^0.11.2",
    "critical": "^1.3.6",
    "del-cli": "^2.0.0",
    "eslint": "^6.3.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "glob": "^7.1.4",
